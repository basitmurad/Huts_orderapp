            android:exported="false"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.OrderActivity"
            android:exported="true"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.HutsActivity"
            android:exported="true"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.CartsActivity"
            android:exported="false"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.SpeciaLOfferActivity"
            android:exported="false"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.FastFoodAndOtherActivity"
            android:exported="false"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.LunchAndDinnerActivity"
            android:exported="false"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.BreakFastActivity"
            android:exported="false"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.SettingActivity"
            android:exported="false"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.ProfileActivity"
            android:exported="false"
            android:theme="@style/Custom.Theme.Huts" />
        <activity
            android:name=".ui.DashboardActivity"
